;; Protocol

(def *protocol* (. :com.github.jlangch.venice.util.Protocol :new))

(def protocol/open (fn [] (do (. *protocol* :clear) nil)))

(def protocol/attach-os (fn [os] (do (. *protocol* :attachOutputStream os) nil)))

(def protocol/log (fn [lvl msg] (do (. *protocol* :log lvl msg) nil)))

(def protocol/to-string (fn [] (str *protocol*)))
